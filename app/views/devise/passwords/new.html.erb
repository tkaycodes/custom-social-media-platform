        <div class="row mainrow">
         
        
          <div class="col-md-8">
            <div class="mainheading" style="width:80%">A picture is worth<br> 10 words.</div><hr id="line" style="width:80%;">
        
            <div class="subheader2">
          <i class="fa fa-instagram fa-3x subheader2icon"></i> 
          <i class="fa fa-twitter fa-3x subheader2icon"></i>      
          <i class="fa fa-comments fa-3x subheader2icon"></i>
            </div>
          </div>

            <div class="col-md-4">
                <div class="login-panel panel panel-default">
                    <!-- <div class="panel-heading">
                        <h3 class="panel-title">Forgot Password?</h3>
                    </div> -->
                    <div class="panel-body">
                     <%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post }) do |f| %>
                      <%= devise_error_messages! %>                           
                      <fieldset>
                          <div class="form-group">
                            <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "E-mail" %>
                          </div>
                          <div class: "actions">
                            <%= f.submit "Send Password Reset Instructions", class: "btn btn-lg btn-success btn-block landingpagebutton squarecorner" %>
                          </div>
                      </fieldset>
                      <% end %>


                      
                      <%= render "devise/shared/links" %>

                    </div>
                </div>
            </div>

        </div>

    </div>

    <script>
        $(document).ready(function(){
          $('.subheader2icon').addClass('animated bounceInUp');
          $('#line').addClass('animated bounceInUp');

          $('.subheader2icon').hover(function(){
            $(this).toggleClass('underlineme')
          });
            // console.log($(this));
